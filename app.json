{
  "expo": {
    "name": "bobee",
    "slug": "bobee",
    "version": "1.0.2",
    "orientation": "portrait",
    "scheme": "bobee",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "backendUrl": "https://bobee-backend.onrender.com"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.bobee"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "splash": {
      "image": "./assets/images/happy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 170,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "react-native-iap",
      "expo-speech-recognition"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
